<div class="col-md-12 col-lg-12">
    @using (Html.BeginForm("", "", FormMethod.Post, new { id = "BusinessCreateReturnForm" }))
    {
        <h1 class="pageTitle">
           Business
        </h1>
        <div class="panel-group" id="accordion">
            <div class="panel panel-default">
                <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapse1">
                    <h4 class="panel-title">
                        Business/Create
                    </h4>
                </div>
                <div id="collapse1" class="panel-collapse collapse in">
                    <div class="panel-body">
                        <table class="responsive">
                            <tr>
                                <td class="labelName"><label class="control-label">Business Name:</label></td>
                                <td class="fieldName">@Html.TextBoxFor(model => model.BusinessNm, new { @class = "form-control" })</td>
                                <td class="labelName"><label style="margin-left:25%" class="control-label">Trade Name:</label></td>
                                <td class="fieldName">@Html.TextBoxFor(model => model.TradeNm, new { @class = "form-control" })</td>
                                <td class="labelName"> <label style="margin-left:25%" class="control-label">EIN or SSN:</label> </td>
                                <td class="fieldName">
                                    @Html.TextBoxFor(model => model.EINorSSN, new { @class = "form-control", @maxlength = "9" })
                                    <span>Is EIN?</span>
                                    @Html.CheckBoxFor(model => model.IsEIN, new { @class = "css-checkbox", id = "chkIsEIN" })
                                </td>
                            </tr>
                            <tr><td class="spacer10"></td></tr>
                            <tr>
                                <td class="labelName"><label class="control-label">Email Address:</label></td>
                                <td class="fieldName">@Html.TextBoxFor(model => model.Email, new { @class = "form-control" })</td>
                                <td class="labelName"><label style="margin-left:25%" class="control-label">Contact Name:</label></td>
                                <td class="fieldName">@Html.TextBoxFor(model => model.ContactNm, new { @class = "form-control" })</td>
                            <tr><td class="spacer10"></td></tr>
                            <tr><td class="spacer10"></td></tr>
                            <tr>
                                <td class="labelName"><label class="control-label">Phone:</label></td>
                                <td class="fieldName">
                                    @Html.TextBoxFor(model => model.Phone, new { @class = "form-control", @maxlength = "10" })
                                </td>
                                <td class="labelName"><label style="margin-left:25%" class="control-label">Phone Extn:</label></td>
                                <td class="fieldName">
                                    @Html.TextBoxFor(model => model.PhoneExtn, new { @class = "form-control", @maxlength = "10" })
                                </td>
                                <td class="labelName"><label style="margin-left:25%" class="control-label">Fax:</label></td>
                                <td class="fieldName">
                                    @Html.TextBoxFor(model => model.Fax, new { @class = "form-control", @maxlength = "10" })
                                </td>
                            </tr>
                            <tr><td class="spacer10"></td></tr>
                            <tr>
                                <td class="labelName"><label class="control-label">Business Type:</label></td>
                                <td>
                                    @Html.DropDownListFor(model => model.BusinessType, Enum.GetValues(typeof(BusinessType)).Cast<BusinessType>().Select(x => new SelectListItem { Text = Utility.GetEnumDisplayName(x), Value = (x).ToString() }), new { @class = "form-control", @onchange = "ChangeBusinessMemberType(this);" })

                                </td>
                                <td class="labelName"><label style="margin-left:25%" class="control-label">Business Member Type:</label></td>
                                <td class="fieldName">@Html.DropDownListFor(model => model.SigningAuthority.BusinessMemberType, Enum.GetValues(typeof(CorporationBusinessMembers)).Cast<CorporationBusinessMembers>().Select(x => new SelectListItem { Text = Utility.GetEnumDisplayName(x), Value = (x).ToString() }), new { @class = "form-control" })</td>
                                <td>
                                    <label style="margin-left:25%" class="control-label">
                                        Is Business Terminated?
                                    </label>
                                </td>
                                <td>
                                    @Html.CheckBoxFor(model => model.IsBusinessTerminated, new { @class = "css-checkbox", id = "chkIsBusinssTerminated" })
                                </td>
                            </tr>
                            <tr><td class="spacer10"></td></tr>
                            <tr>
                                <td class="labelName"><label class="control-label">Signing Authority Name:</label></td>
                                <td class="fieldName">@Html.TextBoxFor(model => model.SigningAuthority.Name, new { @class = "form-control"})</td>
                                <td class="labelName"><label style="margin-left:25%" class="control-label">Signing Authority Phone:</label></td>
                                <td class="fieldName">@Html.TextBoxFor(model => model.SigningAuthority.Phone, new { @class = "form-control", @maxlength = "10" })</td>
                            </tr>

                            <tr><td class="spacer10"></td></tr>
                            <tr>
                                <td class="labelName"><label class="control-label">Kind Of Employer:</label></td>
                                <td class="fieldName">@Html.DropDownListFor(model => model.KindOfEmployer, Enum.GetValues(typeof(KindOfEmployer)).Cast<KindOfEmployer>().Select(x => new SelectListItem { Text = Utility.GetEnumDisplayName(x), Value = (x).ToString() }), new { @class = "form-control" })</td>

                                <td class="labelName"><label style="margin-left:25%" class="control-label">Kind Of Payer:</label></td>
                                <td class="fieldName">@Html.DropDownListFor(model => model.KindOfPayer, Enum.GetValues(typeof(KindOfPayer)).Cast<KindOfPayer>().Select(x => new SelectListItem { Text = Utility.GetEnumDisplayName(x), Value = (x).ToString() }), new { @class = "form-control" })</td>
                            </tr>

                            <tr><td class="spacer10"></td></tr>
                            <tr>
                                <td class="labelName"><label class="control-label">Address 1:</label></td>
                                <td class="fieldName">
                                    @Html.TextBoxFor(model => model.USAddress.Address1, new { @id = "USAddress1", @class = "form-control" })

                                    @Html.TextBoxFor(model => Model.ForeignAddress.Address1, new { @id = "ForeignAddress1", @style = "display:none", @class = "form-control" })
                                </td>
                                <td class="labelName"><label style="margin-left:25%" class="control-label">Address 2:</label></td>
                                <td class="fieldName">
                                    @Html.TextBoxFor(model => model.USAddress.Address2, new { @id = "USAddress2", @class = "form-control" })

                                    @Html.TextBoxFor(model => Model.ForeignAddress.Address2, new { @id = "ForeignAddress2", @style = "display:none", @class = "form-control" })
                                </td>
                                <td>
                                    <span style="margin-left:25%"> Is Foreign?</span>
                                    @Html.CheckBoxFor(model => model.IsForeign, new { onclick = "enableOrDisableFields()", @class = "css-checkbox", id = "IsForeign" })
                                </td>
                            </tr>
                            <tr><td class="spacer10"></td></tr>
                            <tr>
                                <td class="labelName"><label id="City_lbl" class="control-label">City:</label></td>
                                <td class="fieldName">
                                    @Html.TextBoxFor(model => model.USAddress.City, new { @id = "UsaddressCity", @class = "form-control" })
                                    @Html.TextBoxFor(model => model.ForeignAddress.City, new { @id = "ForeginAddressCity", @class = "form-control", @style = "display:none" })
                                </td>
                                <td class="labelName"><label id="state_lbl" style="margin-left:25%" class="control-label">State:</label></td>
                                <td>
                                    @Html.DropDownListFor(model => model.USAddress.State, USStates, new { @id = "State", @class = "form-control" })
                                    @Html.TextBoxFor(model => model.ForeignAddress.ProvinceOrStateNm, new { @id = "ProvinceOrStateNm", @class = "form-control", @style = "display:none" })
                                </td>
                            </tr>
                            <tr><td class="spacer10"></td></tr>
                            <tr>
                                <td class="labelName"><label id="zip_code_lbl" class="control-label">Zip Code:</label></td>
                                <td class="fieldName">
                                    @Html.TextBoxFor(model => model.USAddress.ZipCd, new { @id = "ZipCd", @class = "form-control", @maxlength = "10" })
                                    @Html.TextBoxFor(model => model.ForeignAddress.PostalCd, new { @id = "PostalCd", @class = "form-control", @style = "display:none" })
                                </td>
                                <td class="labelName"><label id="country_lbl" style="display:none;margin-left:25%;" class="control-label">Country:</label></td>
                                <td>
                                    @Html.DropDownListFor(model => Model.ForeignAddress.Country, countryList, "--Select--", new { @id = "country", @class = "form-control", @style = "display:none" })
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    }
  
    <div class="taC">
        <button type="button" class="btn btn_lg btn_primary" onclick="location.href='@Url.Action("Index", "Home")'" id="btnback">
            <i class="fa fa-arrow-circle-left"></i>
            Back
        </button>
        <button type="button" class="btn btn_lg btn_primary" onclick="GetStatus()" id="btnCreate">
            Create Business
            <i class="fa fa-spinner fa-spin" aria-hidden="true" id="CreateSpinner" style="display:none"></i>
        </button>
    </div>
</div>
<script type="text/javascript">
    function enableOrDisableFields() {
        var country = document.getElementById("country");
        country.disabled = document.getElementById("IsForeign").checked ? false : true;
        changeFieldName();
    }

    function changeFieldName() {
        var UsAddress1 = document.getElementById("USAddress1");
        var USAddress2 = document.getElementById("USAddress2");
        var ForeignAddress1 = document.getElementById("ForeignAddress1");
        var ForeignAddress2 = document.getElementById("ForeignAddress2");
        var ForeginAddressCity = document.getElementById("ForeginAddressCity");
        var zipCodeLbl = document.getElementById("zip_code_lbl");
        var zipCodeFld = document.getElementById("ZipCd");
        var ForeignPostalCde = document.getElementById("PostalCd");
        var stateLbl = document.getElementById("state_lbl");
        var stateFld = document.getElementById("ProvinceOrStateNm");
        var USAdressCity = document.getElementById("UsaddressCity");
        var ForeginAdressCity = document.getElementById("ForeginAddressCity");

        var statedropdown = document.getElementById("State");
        var CountryLbl = document.getElementById("country_lbl");
        var CountryFld = document.getElementById("country");

        if (document.getElementById("IsForeign").checked == true) {
            UsAddress1.style.display = "none";
            USAddress2.style.display = "none";
            ForeignAddress1.style.display = "block";
            ForeignAddress2.style.display = "block";
            zipCodeLbl.innerHTML = "Postal Code";
            zipCodeFld.style.display = "none";
            ForeignPostalCde.style.display = "block";
            stateLbl.innerHTML = "Foreign City/State";
            statedropdown.style.display = "none";
            stateFld.style.display = "block";
            ForeginAddressCity.style.display = "block";
            CountryLbl.style.display = "block";
            CountryFld.style.display = "block";
            USAdressCity.style.display = "none";
            ForeginAdressCity.style.display = "block";

        } else {
            UsAddress1.style.display = "block";
            USAddress2.style.display = "block";
            ForeignAddress1.style.display = "none";
            ForeignAddress2.style.display = "none";
            ForeginAddressCity.style.display = "none";
            zipCodeLbl.innerHTML = "Zip Code";
            zipCodeFld.style.display = "block";
            ForeignPostalCde.style.display = "none";
            stateLbl.innerHTML = "State";
            stateFld.style.display = "none";
            statedropdown.style.display = "block";
            CountryLbl.style.display = "none";
            CountryFld.style.display = "none";
            USAdressCity.style.display = "block";
            ForeginAdressCity.style.display = "none";
        }


    }
    function GetStatus() {
        var formdata = $("#BusinessCreateReturnForm").serialize();
        $("#btnCreate > i").show();
        $("#btnCreate").prop("disabled", true);
        $.ajax({
            url: "/Business/CreateBusiness/",
            cache: false,
            type: 'POST',
            data: formdata,
            success: function (data) {
                $(".modal-dialog").addClass("edit-dialog");
                $('#ModelBody').html(data);
                $('#myModal').modal("show");
                $("#btnCreate > i").hide();
                $("#btnCreate").prop("disabled", false);
            },
            error: function (err) {
                console.log(err.responseText);
                $("#btnCreate > i").hide();
                $("#btnCreate").prop("disabled", false);
            }
        });
    }
</script>